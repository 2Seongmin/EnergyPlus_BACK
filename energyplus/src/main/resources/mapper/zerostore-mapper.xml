<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.ecolog.api.model.dao.ZerostoreMapper">

	<select id="requestGetZerostore"
	resultType="ZerostoreDTO">
		SELECT
			ZEROSTORE_SEQ zerostoreSeq,
            STORE_NAME storeName,
            lat,
            lot
		FROM
			TB_ZEROSTORE
	</select>

</mapper>